{
    "manifest_version": 2,
    "name": "page_action",
    "description": "page_action sample extension",
    "version": "0.0",
    "background": {
        "scripts": ["background.js"]
    },
	"content_scripts": [{
		"js": ["content_script.js"],
		"matches": [
			"*://www.expertime.com/*"
		]
	}],
    "page_action": {
        "default_icon": "icon.png",
        "default_title": "page_action sample extension"
    }
}